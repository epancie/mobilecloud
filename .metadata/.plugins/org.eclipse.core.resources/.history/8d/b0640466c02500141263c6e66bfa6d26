package org.pancho.loger;

import java.util.logging.Level;
import java.util.logging.Logger;

public class Loger {
	
	public void _Log(String log) {
	    try {
	         Logger.getLogger(getClass().getName()).log(
	            Level.INFO, "LOG ->" + log);
	       } catch (Exception err) {
	         Logger.getLogger(getClass().getName()).log(
	            Level.SEVERE, "Mensaje crítico...", err);
	       }
	}
}